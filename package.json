{
  "name": "orchestra",
  "version": "1.0.0",
  "description": "Media player, transcoder and streamer",
  "keywords": [
    "decoder",
    "downloader",
    "encoder",
    "multimedia",
    "player",
    "playlist",
    "streamer",
    "transcoder"
  ],
  "homepage": "https://maggiben.github.io/orchestra",
  "bugs": {
    "url": "https://github.com/maggiben/orchestra/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/maggiben/orchestra"
  },
  "license": "MIT",
  "author": {
    "name": "Benjamin Maggi",
    "email": "benjaminmaggi@gmail.com",
    "url": "https://github.com/maggiben"
  },
  "main": "index.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64"
  },
  "build": {
    "appId": "com.devtography.electron_boilerplate",
    "productName": "Electron+React+TypeScript Boilerplate",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "pkg"
    },
    "win": {
      "target": "nsis",
      "asar": false
    },
    "buildVersion": "1.0.0"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "axios": "^0.19.0",
    "codemirror": "^5.58.2",
    "electron": "^7.1.3",
    "jsonata": "^1.7.0",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.12.0",
    "react-intl": "^3.9.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "uuid": "^3.3.3",
    "wavesurfer.js": "^3.2.0",
    "ytdl-core": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@types/ajv": "^1.0.0",
    "@types/chart.js": "^2.7.55",
    "@types/codemirror": "0.0.81",
    "@types/dompurify": "0.0.33",
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.10",
    "@types/jsonata": "^1.5.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.14",
    "@types/react-codemirror": "^1.0.3",
    "@types/react-dom": "^16.9.4",
    "@types/react-intl": "^2.3.18",
    "@types/react-loadable": "^5.5.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/uuid": "^3.4.6",
    "@types/wavesurfer.js": "^2.0.2",
    "axios-mock-adapter": "^1.17.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "cross-env": "^6.0.3",
    "rimraf": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "orchestra": {
    "settings": {
      "file": "settings.json"
    }
  }
}
